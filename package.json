{
  "name": "server",
  "version": "1.0.0",
  "description": "Event Manager",
  "main": "index.js",
  "scripts": {
    "test": "nyc --reporter=html --reporter=text mocha --exit --compilers js:babel-core/register server/v2/test/**/**.spec.js",
    "build": "rimraf dist/ && babel ./server --out-dir dist/ --ignore ./node_modules,./.babelrc,./package.json,./npm-debug.log,./server/dist --copy-files",
    "start": "node dist/index.js",
    "start:dev": "nodemon dist/index.js",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "postinstall": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/omokolataiwo/eventmanager.git"
  },
  "keywords": [
    "Event"
  ],
  "author": "Taiwo Kolawole",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/omokolataiwo/eventmanager/issues"
  },
  "homepage": "https://github.com/omokolataiwo/eventmanager#readme",
  "devDependencies": {
    "chai-http": "^3.0.0",
    "coveralls": "^3.0.0",
    "eslint": "^4.11.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^11.3.0"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.2",
    "chai": "^4.1.2",
    "cors": "^2.8.4",
    "dotenv": "^5.0.1",
    "express": "^4.16.2",
    "express-joi-validator": "^2.0.0",
    "joi": "^13.1.0",
    "jsonwebtoken": "^8.1.0",
    "moment": "^2.19.3",
    "nodemailer": "^4.6.4",
    "pg": "^7.4.0",
    "pg-hstore": "^2.3.2",
    "rimraf": "^2.6.2",
    "sequelize": "^4.23.1",
    "validate.js": "^0.12.0",
    "validator": "^9.1.1"
  }
